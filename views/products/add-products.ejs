<!doctype html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>SanePharma </title>
        
        
        
    </head>        
<body>

    

     
     
  <form class="form-inline" role="form" style="position:absolute; top:-60px; z-index:-1;" method="post" action="/addproducts">
    
    <table border="0">
        <tr>
            <td height="100" width="350">
                 <div class="form-group">
                      <label for="pcode">Product Code:</label>
                      <input type="text" class="form-control" name="pcode" placeholder="Enter Unique code">
                 </div>
            </td>
            
            <td>
                <div class="form-group">
                      <label for="pname">Product Name:</label>
                      <input type="text" class="form-control" name="pname" placeholder="Enter Product Name">
                </div>
            </td>
            
            <td>
                <div class="form-group">
                      <label for="ppacking">Packing:</label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                      <input type="text" class="form-control" name="ppacking" placeholder="Packing details">
                </div>  
            </td>
        </tr>    
          </br></br></br></br>
    
        
        <tr>
    
            <td height="100" width="350">
                <div class="form-group">
                      <label for="pcompany">Company:</label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                      <input type="text" class="form-control" name="pcompany" placeholder="Enter Company Name">
                </div>
            </td>
            
            <td>
            
                <div class="form-group">
                      <label for="psalestax">Sales Tax as :</label>&nbsp;&nbsp;
                      <input type="text" class="form-control" name="psalestax" value="10%">
                </div>
            </td>
            
            <td>
            
                <div class="form-group">
                      <label for="pdiscount">Purchase Discount:</label>&nbsp;&nbsp;&nbsp;&nbsp;
                      <input type="text" class="form-control" name="pdiscount" value="0%">
                </div>  
            </td>
        </tr>
        </br></br></br></br>

        <tr>
            <td height="100" width="350">
                <div class="form-group">
                  <label for="pboxsize">Box/Case Size:</label>
                  <input type="text" class="form-control" name="pboxsize" placeholder="10' X 6'">
                </div>
            </td> 
            <td width="350">
                <div class="form-group">
                  <label for="ppurchase">Purchase as :</label>&nbsp;&nbsp;
                  <input type="text" class="form-control" name="ppurchase" placeholder="Purchase Cost">
                </div>
            </td>
            <td>
                <div class="form-group">
                  <label for="pmrp">Maximum Retail Price :</label>
                  <input type="text" class="form-control" name="pmrp" placeholder="Rs . MRP">
                </div>  
            </td>
        </tr>    
  </table>  
    </br></br></br></br>
    <button type="submit" class="btn btn-primary" data-toggle="modal" data-target="#myModal">Save</button>
    <button type="reset" class="btn btn-warning">Reset</button>
   
  </form>

 


    <div class="modal fade" id="myModal">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title">Success</h4>
          </div>
          <div class="modal-body">
            <p>Your data has been saved successfully !</p>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>

          </div>
        </div><!-- /.modal-content -->
      </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->



    <div class="modal fade" id="myModal2">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title">About</h4>
          </div>
          <div class="modal-body">
            <p>The Service is built by Specta</p>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>

          </div>
        </div><!-- /.modal-content -->
      </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->





</div>    
    
    
</body>
</html>

